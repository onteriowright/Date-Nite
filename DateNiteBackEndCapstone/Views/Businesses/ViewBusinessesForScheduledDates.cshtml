@model DateNiteBackEndCapstone.Models.BusinessViewModals.BusinessListViewModel

@{
    ViewData["Title"] = "ViewScheduledDates";
}

<h1>Places For This Date</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                Business
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Businesses)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
            </tr>
        }
    </tbody>
</table>
<td>
    @Html.ActionLink("Back", "ScheduledDates", "Dates")
</td>